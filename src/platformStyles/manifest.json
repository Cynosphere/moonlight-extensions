{
  "$schema": "https://moonlight-mod.github.io/manifest.schema.json",
  "id": "platformStyles",
  "version": "1.0.1",
  "meta": {
    "name": "Platform Styles",
    "tagline": "Change the window border apperance to another operating system's style.",
    "authors": [
      "Cynosphere",
      "adryd"
    ]
  },
  "settings": {
    "style": {
      "displayName": "Style (linux for system border)",
      "type": "select",
      "default": "default",
      "options": [
        "default",
        "win",
        "osx",
        "linux",
        "web"
      ]
    }
  },
  "apiLevel": 2
}
