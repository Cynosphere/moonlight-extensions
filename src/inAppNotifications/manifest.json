{
  "$schema": "https://moonlight-mod.github.io/manifest.schema.json",
  "id": "inAppNotifications",
  "version": "1.0.5",
  "meta": {
    "name": "In App Notifications",
    "tagline": "In app notifications akin to mobile",
    "authors": ["Cynosphere"],
    "tags": ["qol", "chat"],
    "source": "https://github.com/Cynosphere/moonlight-extensions",
    "donate": "https://ko-fi.com/Cynosphere",
    "changelog": "Attempt to fix loading issues I cannot replicate"
  },
  "dependencies": ["spacepack", "common"],
  "settings": {
    "replace": {
      "displayName": "New notifications replace existing",
      "description": "Instead of stacking and having to wait for each notification",
      "type": "boolean",
      "default": false,
      "advice": "none"
    }
  },
  "apiLevel": 2
}
